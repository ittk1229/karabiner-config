    {
      "description": "vim-like arrows: map ctrl + h/j/k/l to ←/↓/↑/→",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["left_shift", "right_shift", "left_gui", "right_gui", "left_alt", "right_alt"]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["left_shift", "right_shift", "left_gui", "right_gui", "left_alt", "right_alt"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["left_shift", "right_shift", "left_gui", "right_gui", "left_alt", "right_alt"]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["right_control"],
              "optional": ["left_shift", "right_shift", "left_gui", "right_gui", "left_alt", "right_alt"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ]
        }
      ]
    },
