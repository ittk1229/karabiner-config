{
  "title": "for ittk1229",
  "rules": [
    {
      "description": "escape for japanese vimmer: map ctrl + [ to escape → japanese_eisuu",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["any"]
            }
          },
          "to": [{ "key_code": "escape" }, { "key_code": "japanese_eisuu" }]
        }
      ]
    },
    {
      "description": "vim-like arrows: map ctrl + h/j/k/l to ←/↓/↑/→",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["left_shift", "right_shift", "left_gui", "right_gui", "left_alt", "right_alt"]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["left_shift", "right_shift", "left_gui", "right_gui", "left_alt", "right_alt"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["left_shift", "right_shift", "left_gui", "right_gui", "left_alt", "right_alt"]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["left_shift", "right_shift", "left_gui", "right_gui", "left_alt", "right_alt"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ]
        }
      ]
    },
    {
      "description": "corne-like keys: map ctrl + japanese_kana/right_command to enter/backspacd",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "japanese_kana",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["any"]
            }
          },
          "to": [{ "key_code": "return_or_enter" }]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "right_command",
            "modifiers": {
              "mandatory": ["control"],
              "optional": ["any"]
            }
          },
          "to": [{ "key_code": "delete_or_backspace" }]
        }
      ]
    },
    {
